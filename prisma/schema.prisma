generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

model students {
  rollno String  @id @db.VarChar(20)
  name   String? @db.VarChar(50)
  dept   String? @db.VarChar(30)
}
