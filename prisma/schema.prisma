model admins {
  id        BigInt  @id @default(autoincrement())
  user_name String? @db.VarChar(100)
  password  String? @db.VarChar(100)
}

model auth_group {
  id                     Int                      @id @default(autoincrement())
  name                   String                   @unique(map: "name") @db.VarChar(150)
  auth_group_permissions auth_group_permissions[]
  students_groups        students_groups[]
}

model auth_group_permissions {
  id              BigInt          @id @default(autoincrement())
  group_id        Int
  permission_id   Int
  auth_group      auth_group      @relation(fields: [group_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "auth_group_permissions_group_id_b120cbf9_fk_auth_group_id")
  auth_permission auth_permission @relation(fields: [permission_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "auth_group_permissio_permission_id_84c5c92e_fk_auth_perm")

  @@unique([group_id, permission_id], map: "auth_group_permissions_group_id_permission_id_0cd325b0_uniq")
  @@index([permission_id], map: "auth_group_permissio_permission_id_84c5c92e_fk_auth_perm")
}

model auth_permission {
  id                        Int                         @id @default(autoincrement())
  name                      String                      @db.VarChar(255)
  content_type_id           Int
  codename                  String                      @db.VarChar(100)
  django_content_type       django_content_type         @relation(fields: [content_type_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "auth_permission_content_type_id_2f476e4b_fk_django_co")
  auth_group_permissions    auth_group_permissions[]
  students_user_permissions students_user_permissions[]

  @@unique([content_type_id, codename], map: "auth_permission_content_type_id_codename_01ab375a_uniq")
}

model django_admin_log {
  id                  Int                  @id @default(autoincrement())
  action_time         DateTime             @db.DateTime(6)
  object_id           String?              @db.LongText
  object_repr         String               @db.VarChar(200)
  action_flag         Int                  @db.UnsignedSmallInt
  change_message      String               @db.LongText
  content_type_id     Int?
  user_id             BigInt
  django_content_type django_content_type? @relation(fields: [content_type_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "django_admin_log_content_type_id_c4bce8eb_fk_django_co")
  students            students             @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "django_admin_log_user_id_c564eba6_fk_students_id")

  @@index([content_type_id], map: "django_admin_log_content_type_id_c4bce8eb_fk_django_co")
  @@index([user_id], map: "django_admin_log_user_id_c564eba6_fk_students_id")
}

model django_content_type {
  id               Int                @id @default(autoincrement())
  app_label        String             @db.VarChar(100)
  model            String             @db.VarChar(100)
  auth_permission  auth_permission[]
  django_admin_log django_admin_log[]

  @@unique([app_label, model], map: "django_content_type_app_label_model_76bd3d3b_uniq")
}

model django_migrations {
  id      BigInt   @id @default(autoincrement())
  app     String   @db.VarChar(255)
  name    String   @db.VarChar(255)
  applied DateTime @db.DateTime(6)
}

model django_session {
  session_key  String   @id @db.VarChar(40)
  session_data String   @db.LongText
  expire_date  DateTime @db.DateTime(6)

  @@index([expire_date], map: "django_session_expire_date_a5c62663")
}

model drf_api_logs {
  id                BigInt   @id @default(autoincrement())
  added_on          DateTime @db.DateTime(6)
  api               String   @db.VarChar(1024)
  headers           String   @db.LongText
  body              String   @db.LongText
  method            String   @db.VarChar(10)
  client_ip_address String   @db.VarChar(50)
  response          String   @db.LongText
  status_code       Int      @db.UnsignedSmallInt
  execution_time    Decimal  @db.Decimal(8, 5)

  @@index([method], map: "drf_api_logs_method_f610a90b")
  @@index([status_code], map: "drf_api_logs_status_code_4ffd4334")
}

model eligible_students {
  id             BigInt       @id @default(autoincrement())
  job_opening_id BigInt
  student_id     BigInt
  job_openings   job_openings @relation(fields: [job_opening_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "eligible_students_job_opening_id_cdf246c8_fk_job_openings_id")
  students       students     @relation(fields: [student_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "eligible_students_student_id_0d78fa18_fk_students_id")

  @@index([job_opening_id], map: "eligible_students_job_opening_id_cdf246c8_fk_job_openings_id")
  @@index([student_id], map: "eligible_students_student_id_0d78fa18_fk_students_id")
}

model job_openings {
  id                BigInt              @id @default(autoincrement())
  email             String?             @db.VarChar(50)
  batch             Int?
  valid_till        String?             @db.VarChar(50)
  branch            String?             @db.VarChar(50)
  offers            String?             @db.VarChar(50)
  tenth_percent     Decimal?            @db.Decimal(6, 2)
  twelveth_percent  Decimal?            @db.Decimal(6, 2)
  diploma_percent   Decimal?            @db.Decimal(6, 2)
  be_percent        Decimal?            @db.Decimal(6, 2)
  cgpa              Decimal?            @db.Decimal(6, 2)
  live_kt           Int?
  dead_kt           Int?
  notice            String?             @db.LongText
  gap               Int?
  package           String?             @db.VarChar(50)
  eligible_students eligible_students[]
}

model other_info {
  id                   BigInt    @id @default(autoincrement())
  hobbies              String?   @db.VarChar(1000)
  pos_of_res_one       String?   @db.VarChar(50)
  pos_of_res_two       String?   @db.VarChar(50)
  pos_of_res_three     String?   @db.VarChar(50)
  pos_of_res_four      String?   @db.VarChar(50)
  extracuricular_one   String?   @db.VarChar(500)
  extracuricular_two   String?   @db.VarChar(500)
  extracuricular_three String?   @db.VarChar(500)
  student_id           BigInt?
  students             students? @relation(fields: [student_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "other_info_student_id_8a20c5be_fk_students_id")

  @@index([student_id], map: "other_info_student_id_8a20c5be_fk_students_id")
}

model student_experience {
  id               BigInt    @id @default(autoincrement())
  internship_one   String?   @db.VarChar(1000)
  internship_two   String?   @db.VarChar(1000)
  internship_three String?   @db.VarChar(1000)
  project_one      String?   @db.VarChar(1000)
  project_two      String?   @db.VarChar(1000)
  project_three    String?   @db.VarChar(1000)
  pref_lang        String?   @db.VarChar(50)
  technologies     String?   @db.VarChar(1000)
  student_id       BigInt?
  students         students? @relation(fields: [student_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "student_experience_student_id_69de8afc_fk_students_id")

  @@index([student_id], map: "student_experience_student_id_69de8afc_fk_students_id")
}

model student_placement_details {
  id               BigInt    @id @default(autoincrement())
  offer_letter_one String    @db.VarChar(100)
  offer_letter_two String    @db.VarChar(100)
  placed_org_one   String?   @db.VarChar(50)
  placed_org_two   String?   @db.VarChar(50)
  package_one      Decimal?  @db.Decimal(12, 4)
  package_two      Decimal?  @db.Decimal(12, 4)
  student_id       BigInt?
  students         students? @relation(fields: [student_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "student_placement_details_student_id_19125985_fk_students_id")

  @@index([student_id], map: "student_placement_details_student_id_19125985_fk_students_id")
}

model student_skillset {
  id                     BigInt    @id @default(autoincrement())
  certificate_one        String    @db.VarChar(100)
  certificate_two        String    @db.VarChar(100)
  certificate_three      String    @db.VarChar(100)
  certificate_four       String    @db.VarChar(100)
  acad_achievement_one   String?   @db.VarChar(50)
  acad_achievement_two   String?   @db.VarChar(50)
  acad_achievement_three String?   @db.VarChar(50)
  acad_achievement_four  String?   @db.VarChar(50)
  career_obj             String?   @db.VarChar(1000)
  student_id             BigInt?
  students               students? @relation(fields: [student_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "student_skillset_student_id_5091c8e4_fk_students_id")

  @@index([student_id], map: "student_skillset_student_id_5091c8e4_fk_students_id")
}

model students {
  id                        BigInt                      @id @default(autoincrement())
  roll_no                   String                      @unique(map: "roll_no") @db.VarChar(10)
  first_name                String?                     @db.VarChar(50)
  middle_name               String?                     @db.VarChar(50)
  last_name                 String?                     @db.VarChar(50)
  email                     String?                     @unique(map: "students_email_4bd9aec2_uniq") @db.VarChar(50)
  phone_number              String?                     @db.VarChar(20)
  gender                    String?                     @db.VarChar(10)
  github                    String?                     @db.VarChar(50)
  linkedin                  String?                     @db.VarChar(50)
  no_of_offers              Int?
  password                  String                      @db.VarChar(100)
  photo                     String                      @db.VarChar(100)
  department                String?                     @db.VarChar(50)
  batch                     Int?
  rait_email                String?                     @unique(map: "students_rait_email_37675066_uniq") @db.VarChar(50)
  is_superuser              Boolean
  last_login                DateTime?                   @db.DateTime(6)
  is_staff                  Boolean
  academic_info             academic_info[]
  django_admin_log          django_admin_log[]
  eligible_students         eligible_students[]
  other_info                other_info[]
  student_experience        student_experience[]
  student_placement_details student_placement_details[]
  student_skillset          student_skillset[]
  students_groups           students_groups[]
  students_user_permissions students_user_permissions[]
}

model students_groups {
  id         BigInt     @id @default(autoincrement())
  student_id BigInt
  group_id   Int
  auth_group auth_group @relation(fields: [group_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "students_groups_group_id_04f6e2cc_fk_auth_group_id")
  students   students   @relation(fields: [student_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "students_groups_student_id_8b9a8731_fk_students_id")

  @@unique([student_id, group_id], map: "students_groups_student_id_group_id_6e0fd69a_uniq")
  @@index([group_id], map: "students_groups_group_id_04f6e2cc_fk_auth_group_id")
}

model students_user_permissions {
  id              BigInt          @id @default(autoincrement())
  student_id      BigInt
  permission_id   Int
  auth_permission auth_permission @relation(fields: [permission_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "students_user_permis_permission_id_7525e7e1_fk_auth_perm")
  students        students        @relation(fields: [student_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "students_user_permissions_student_id_ecd305e7_fk_students_id")

  @@unique([student_id, permission_id], map: "students_user_permissions_student_id_permission_id_e35193b9_uniq")
  @@index([permission_id], map: "students_user_permis_permission_id_7525e7e1_fk_auth_perm")
}
